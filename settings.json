{
  //Main editor settings
  "editor.folding": false,
  "editor.insertSpaces": false,
  "editor.smoothScrolling": true,
  "editor.minimap.enabled": false,
  //Appearance
  "editor.bracketPairColorization.enabled": false,
  "editor.glyphMargin": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.accessibilitySupport": "off",
  "editor.scrollbar.vertical": "hidden",
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  //Breadcrumbs
  "breadcrumbs.icons": false,
  "breadcrumbs.showKeys": false,
  "breadcrumbs.showFiles": false,
  "breadcrumbs.symbolPath": "off",
  "breadcrumbs.showArrays": false,
  "breadcrumbs.showEvents": false,
  "breadcrumbs.showFields": false,
  "breadcrumbs.showClasses": false,
  "breadcrumbs.showMethods": false,
  "breadcrumbs.showBooleans": false,
  "breadcrumbs.showFunctions": false,
  "breadcrumbs.showConstants": false,
  "breadcrumbs.showEnumMembers": false,
  "breadcrumbs.showConstructors": false,
  //Formatters
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always"
  },
  "[svelte]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[vue]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }, 
  "editor.tabSize": 2,
  "parameterHints.hintingType": "variableOnly",
  "dart.flutterSdkPath": "/Users/user/mobile-softphone/flutter",
  "editor.renderWhitespace": "none",
  "editor.fontWeight": "300",
  "editor.fontSize": 17,
  "editor.lineHeight": 1.3,
  "nxConsole.showNodeVersionOnStartup": false,
  "editor.guides.indentation": false,
  "typescript.tsserver.log": "verbose",
  "editor.quickSuggestionsDelay": 0,
  "svelte.enable-ts-plugin": true,
  "editor.cursorStyle": "block",
  "editor.fontFamily": "Fira Code, JetBrains Mono, Space Mono, Menlo, Monaco, 'Courier New', monospace, Hack Nerd Font Propo",
  "editor.fontLigatures": "'calt', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss07', 'ss08', 'ss10', 'zero', 'cv31', 'cv32', 'cv27', 'cv24', 'cv26', 'cv29', 'cv30', 'cv16'",
  "terminal.integrated.fontFamily": "Fira Code, JetBrains Mono,  Hack Nerd Font Propo, Space Mono",
  "scm.inputFontFamily": "Fira Code, JetBrains Mono, Space Mono",
  "notebook.output.fontFamily": "Fira Code, JetBrains Mono, Space Mono",
  "chat.editor.fontFamily": "Fira Code, JetBrains Mono, Space Mono",
  "debug.console.fontFamily": "Fira Code, JetBrains Mono, Space Mono",
  "editor.inlineSuggest.fontFamily": "Fira Code, JetBrains Mono, Space Mono",
  "editor.indentSize": "tabSize",
  "update.mode": "manual",
  //Apc extension
  "apc.electron": {
    "opacity": 0.8,
    "transparent": true,
    "vibrancy": "ultra-dark",
    "titleBarStyle": "hidden",
    "titleBarOverlay": {
      "height": 0
    }
  },
  "apc.font.family": "Fira Code",
  "apc.header": {
    "height": 5
  },
  "apc.listRow": {
    "lists": [
      "explorer-folders-view"
    ],
    "height": 50
  },
  "apc.statusBar": {
    "position": "editor-bottom"
  },
  "prisma.showPrismaDataPlatformNotification": false,
  "diffEditor.ignoreTrimWhitespace": false,
  // VSCODE performance settings
  "editor.codeLens": false,
  "search.searchOnType": false,
  "workbench.colorTheme": "Tokyo Night Blackout",
  "workbench.activityBar.location": "top",
  "terminal.external.osxExec": "Warp.app",
  "terminal.integrated.env.osx": {
    "Q_NEW_SESSION": "1"
  },
  "eslint.probe": [
    "astro",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "mdx",
    "vue",
    "markdown",
    "json",
    "jsonc",
    "svelte"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "svelte",
    "vue"
  ],
  "workbench.iconTheme": "Monokai Classic Icons",
  "editor.codeLensFontFamily": "Fira Code",
  "editor.inlayHints.fontFamily": "Fira Code",
  "editor.minimap.sectionHeaderFontSize": 8,
  "apc.monospace.font.family": "Fira Code",

  // custom-ui-style
  "custom-ui-style.font.sansSerif": "Fira Code",
  "accessibility.voice.speechTimeout": 2000,
  "accessibility.voice.speechLanguage": "ru-RU",
}
